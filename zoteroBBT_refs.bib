@article{wang_aluminum_nodate,
  title = {Aluminum {{Chloride}}-{{Graphite Batteries}} with {{Flexible Current Collectors Prepared}} from {{Earth}}-{{Abundant Elements}}},
  issn = {2198-3844},
  abstract = {In the search for low-cost and large-scale stationary storage of electricity, nonaqueous aluminum chloride-graphite batteries (AlCl3-GBs) have received much attention due to the high natural abundances of their primary constituents, facile manufacturing, and high energy densities. Much research has focused on the judicious selection of graphite cathode materials, leading to the most notable recent advances in the performance of AlCl3-GBs. However, the major obstacle to commercializing this technology is the lack of oxidatively stable, inexpensive current collectors that can operate in chloroaluminate ionic liquids and are composed of earth-abundant elements. This study presents the use of titanium nitride (TiN) as a compelling material for this purpose. Flexible current collectors can be fabricated by coating TiN on stainless steel or flexible polyimide substrates by low-cost, rapid, scalable methods such as magnetron sputtering. When these current collectors are used in AlCl3-GB coin or pouch cells, stable cathodic operation is observed at voltages of up to 2.5 V versus Al3+/Al. Furthermore, these batteries have a high coulombic efficiency of 99.5\%, power density of 4500 W kg-1, and cyclability of at least 500 cycles.},
  language = {en},
  journal = {Advanced Science},
  doi = {10.1002/advs.201700712},
  author = {Wang, Shutao and Kravchyk, Kostiantyn V. and Filippin, Alejandro N. and M{\"u}ller, Ulrich and Tiwari, Ayodhya N. and Buecheler, Stephan and Bodnarchuk, Maryna I. and Kovalenko, Maksym V.},
  keywords = {graphite,energy density,aluminum chloride graphite batteries,current collectors,titanium nitride},
  pages = {n/a-n/a},
  file = {/Users/tn438/Zotero/storage/UCGBGE3D/Wang et al. - Aluminum Chloride-Graphite Batteries with Flexible.pdf;/Users/tn438/Zotero/storage/265BLX2X/abstract.html}
}

