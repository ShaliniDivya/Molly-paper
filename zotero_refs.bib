	year = {2008},
	pages = {7}
}

@article{xu_rapid_2008,
	title = {Rapid synthesis of highly luminescent {InP} and {InP}/{ZnS} nanocrystals},
	volume = {18},
	copyright = {All rights reserved},
	issn = {0959-9428},
	url = {http://dx.doi.org/10.1039/B803263G},
	doi = {10.1039/B803263G},
	abstract = {We report a simple method for the fast synthesis of highly photoluminescent InP and InP/ZnS core-shell nanocrystals (NCs) covering a wide range of emissions from blue to the near infrared. Both InP and InP/ZnS NCs were prepared in one-step, in a one-pot reaction within 20 min using an InCl complex as a precursor and zinc carboxylate as an initiator and stabilizer. The quantum yields of the InP and InP/ZnS NCs were 30\% and 60\%, respectively. This new synthetic method allows fast and reproducible preparation of InP and InP/ZnS NCs with a quality comparable to that of the frequently used CdSe-based ones. It is anticipated that these particles can be used to replace CdSe for many applications.},
