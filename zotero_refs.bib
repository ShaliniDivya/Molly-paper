@article{chen_development_2012,
	title = {Development of {Hydrophilic} {Iron} {Oxide} {Nanocubes}},
	volume = {4},
	url = {http://www.ingentaconnect.com/content/asp/sam/2012/00000004/00000008/art00013},
	doi = {10.1166/sam.2012.1357},
	abstract = {Iron oxide nanoparticles with superparamagnetic properties (SIONPs) have demonstrated the great capability to separate heavy metals from contaminated aqueous system, e.g., river, ground water, etc. To date, thermal decomposition methods are commonly used to synthesize highly crystalline monodisperse SIONPs with various shapes (e.g., spherical and cubic). However, the produced SIONPs show very poor dispersibility and stability in aqueous media. Thus, it is important to produce hydrophilic SIONPs by modifying the surface of iron oxide nanoparticles. Here, highly mono-dispersed iron oxide nanocubes (IONCs) were synthesized by a modified thermal decomposition approach. Following that, the oleic acid ligand was then replaced by 11-Mercaptoundecanoic acid (MUA) through a ligand exchange method. By control the growth of crystals, we are able to produce nanocubes that can be well dispersed in aqueous media. The average length for the edges of IONCs was estimated at 8 nm. The successful ligand replacement was proved by the spectra of FTIR. Furthermore, the thiol group presenting on the surface of nanocubes enable the functionalized IONCs as the excellent sorbent materials for extraction of heavy metal and actinide contaminants from environmental and clinical samples.},
	number = {8},
	journal = {Science of Advanced Materials},
	author = {Chen, Longyan and Zhang, Jin},
	year = {2012},
	keywords = {IRON OXIDE NANOPARTICLES, LIGAND EXCHANGE, NANOCUBE, SUPERPARAMAGNETIC, WATER DISPERSIBILITY},
	pages = {859--862}
}

