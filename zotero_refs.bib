	author = {Asakura, Daisuke and Okubo, Masashi and Mizuno, Yoshifumi and Kudo, Tetsuichi and Zhou, Haoshen and Amemiya, Kenta and de Groot, Frank M. F. and Chen, Jeng-Lung and Wang, Wei-Cheng and Glans, Per-Anders and Chang, Chinglin and Guo, Jinghua and Honma, Itaru},
