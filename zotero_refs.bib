@article{kruger_coordination_2013,
	title = {Coordination {Polymers} and {Metal}–{Organic} {Frameworks} {Derived} from 4,4'-{Dicarboxy}-2,2'-bipyridine and 4,4',6,6'-{Tetracarboxy}-2,2'-bipyridine {Ligands}: {A} {Personal} {Perspective}},
