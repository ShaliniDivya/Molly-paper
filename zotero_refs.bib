@article{ayupova_vitro_2019,
	title = {An {In} {Vitro} {Investigation} of {Cytotoxic} {Effects} of {InP}/{Zns} {Quantum} {Dots} with {Different} {Surface} {Chemistries}},
	volume = {9},
	issn = {2079-4991},
	doi = {10.3390/nano9020135},
	abstract = {Indium phosphide quantum dots (QDs) passivated with zinc sulphide in a core/shell architecture (InP/ZnS) with different surface chemistries were introduced to RAW 264.7 murine \&ldquo;macrophage-like\&rdquo; cells to understand their potential toxicities. The InP/ZnS quantum dots were conjugated with an oligonucleotide, a carboxylic acid, or an amino-polyethylene glycol ligand, and cell viability and cell proliferation were investigated via a metabolic assay. Membrane integrity was measured through the production of lactate dehydrogenase. Fluorescence microscopy showed cellular uptake. All quantum dots exhibited cytotoxic behaviour less than that observed from cadmium- or lead-based quantum dots; however, this behaviour was sensitive to the ligands used. In particular, the amino-polyethylene glycol conjugated quantum dots proved to possess the highest cytotoxicity examined here. This provides quantitative evidence that aqueous InP/ZnS quantum dots can offer a safer alternative for bioimaging or in therapeutic applications.},
	number = {2},
	journal = {Nanomaterials},
	author = {Ayupova, Deanna and Dobhal, Garima and Laufersky, Geoffry and Nann, Thomas and Goreham, V. Renee},
	year = {2019},
	keywords = {aptamer, bioimaging, cytotoxicity, photoluminescence, quantum dots, quantum yields}
}

@incollection{laufersky_1.01_2019,
	address = {Oxford},
	title = {1.01 - {Physical} {Chemistry} of {Nanoparticle} {Syntheses}☆},
	isbn = {978-0-12-812296-9},
	url = {http://www.sciencedirect.com/science/article/pii/B9780128035818113426},
	abstract = {The past several decades have seen tremendous growth in the synthesis and applications of nanomaterials in countless areas of research. As such, many current reaction systems are well established in terms of synthetic protocols and mechanistic understanding and can allow non-specialists to reliably generate a wide range of nanoparticles. However, altering existing methods or developing syntheses with new precursors can dramatically change reaction outcomes; systems often have to be optimized in seemingly unintuitive ways to generate quality products. Herein, we outline some of the main physical chemical processes involved with nanoparticle nucleation and growth in an effort to shed light on the consequences of reaction designs. Focus is spent on tactics for producing monodisperse nanomaterials and tuning reactions to generate different nanoparticle sizes, with resources included to assist with more specific reaction systems.},
	booktitle = {Comprehensive {Nanoscience} and {Nanotechnology} ({Second} {Edition})},
	publisher = {Academic Press},
