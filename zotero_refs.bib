@article{ren_regenerative_2011,
	title = {Regenerative {Methanol} {Fuel} {Cells}: {Reduction} of {CO}2 to {Methanol} on {Oxidized} {Cu} {Electrodes}},
	volume = {33},
	url = {http://ecst.ecsdl.org/content/33/39/253.abstract},
	abstract = {Electrochemical reduction of CO2 was performed via oxidized Cu electrodes in an aqueous KHCO3 solution saturated with CO2. Although the formal potentials for reducing Cu oxides are much higher (E = 0.522 V (SHE) for Cu+ and E = 0.340 V (SHE) for Cu2+) than CO2 reduction, CO2 reduction products are observed for short time periods ({\textless}10 minutes) in batch reactors. Electrodeposited Cu(I) electrodes showed the highest CH3OH yields near 43 µmol cm-2 hr-1 with Faradaic efficiencies near 38\%.   The work suggests oxide interfaces play a key role in product selectivity and lends insight into characteristics needed for stable electrocatalyts for direct CH3OH generation from CO2.},
	number = {39},
	journal = {ECS Transactions},
	author = {Ren, Maoming and Le, Minh and Zhang, Ziyu and Sprunger, Phillip T. and Flake, J. C.},
